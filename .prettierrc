{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "trailingComma": "all",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/services",
    "^@/types",
    "^@/utils",
    "^@/middlewares",
    "^@/modules",
    "^@/guards",
    "^@/constants",
    "^(.*module)$",
    "^(.*controller)$",
    "^(.*service)$",
    "^(.*interface)$",
    "^(.*dto)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["typescript", "decorators-legacy"]
}
